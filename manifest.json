{
    "name": "Dipak's Chrome Mods",
    "version": "1.0",
    "description": "The Way I like somethings :)",
    "permissions": [],
    "icons": {
      "16": "images/icons/icon-16x16.png",
      "32": "images/icons/icon-32x32.png",
      "48": "images/icons/icon-48x48.png",
      "128": "images/icons/icon-128x128.png"
    },
    "content_scripts": [
      {
        "matches": [
          "*://mail.google.com/*"
        ],
        "js": [
          "customize-mail.js"
        ]
      },
      {
        "matches": [
          "*://facebook.com/*",
          "*://www.facebook.com/*"
        ],
        "js": [
          "customize-facebook.js"
        ]
      }
      
    ],
    "manifest_version": 2
  }